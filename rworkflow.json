{
    "ComputeServers": {
        "GCP": {
            "FaaSType": "GoogleCloud",
            "Namespace": "faasr-dev",
            "Region": "us-central1",
            "Endpoint": "https://run.googleapis.com/v2/projects/",
            "UseSecretStore": true,
            "ClientEmail": "faasr-workflow-runner@faasr-dev.iam.gserviceaccount.com",
            "TokenUri": "https://oauth2.googleapis.com/token",
            "CPUsPerTask": 1,
            "Memory": 512,
            "TimeLimit": 3600
        }
    },
    "DataStores": {
        "My_S3_Bucket": {
            "Endpoint": "https://play.min.io",
            "Bucket": "faasr",
            "Region": "us-east-1"
        }
    },
    "ActionList": {
        "rfunc": {
            "FunctionName": "myfunction",
            "FaaSServer": "GCP",
            "Type": "R",
            "Arguments": {
                "message": "testing conditional branching"
            },
            "InvokeNext": [
                {
                    "True": ["rfunc-true"],
                    "False": ["rfunc-false"]
                }
            ]
        },
        "rfunc-true": {
            "FunctionName": "r_log",
            "FaaSServer": "GCP",
            "Type": "R",
            "Arguments": {
                "message": "This executed because rfunc returned TRUE"
            },
            "InvokeNext": ["rfunc-final"]
        },
        "rfunc-false": {
            "FunctionName": "r_log",
            "FaaSServer": "GCP",
            "Type": "R",
            "Arguments": {
                "message": "This executed because rfunc returned FALSE"
            },
            "InvokeNext": ["rfunc-final"]
        },
         "rfunc-final": {
            "FunctionName": "r_log",
            "FaaSServer": "GCP",
            "Type": "R",
            "Arguments": {
                "message": "This executed because rfunc returned FALSE"
            },
            "InvokeNext": []
        }
    },
    "ActionContainers": {
        "rfunc": "ramrakha/gcp-r:dev",
        "rfunc-true": "ramrakha/gcp-r:dev",
        "rfunc-false": "ramrakha/gcp-r:dev",
        "rfunc-final":"ramrakha/gcp-r:dev"
    },
    "FunctionCRANPackage": {
        "r_log": [
            "gapminder"
        ]
    },
    "FunctionGitRepo": {
        "r_log": "Ashish-Ramrakhiani/test-py",
        "myfunction":"Ashish-Ramrakhiani/FaaSr-worfklow-public"
    },
    "FunctionInvoke": "rfunc",
    "FaaSrLog": "FaaSrLog",
    "LoggingDataStore": "My_S3_Bucket",
    "DefaultDataStore": "My_S3_Bucket",
    "InvocationID": "1234",
    "WorkflowName": "test-gcp-conditional"
}
