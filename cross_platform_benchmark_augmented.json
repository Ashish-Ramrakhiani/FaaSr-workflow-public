{
    "ComputeServers": {
        "GH": {
            "FaaSType": "GitHubActions",
            "UseSecretStore": true,
            "UserName": "Ashish-Ramrakhiani",
            "ActionRepoName": "FaaSr-workflow-public"
        },
        "GCP": {
            "FaaSType": "GoogleCloud",
            "Namespace": "faasr-dev",
            "Region": "us-central1",
            "Endpoint": "https://run.googleapis.com/v2/projects/",
            "UseSecretStore": true,
            "ClientEmail": "faasr-workflow-runner@faasr-dev.iam.gserviceaccount.com",
            "TokenUri": "https://oauth2.googleapis.com/token",
            "CPUsPerTask": 1,
            "Memory": 512,
            "TimeLimit": 3600
        },
        "AWS": {
            "FaaSType": "Lambda",
            "Region": "us-east-1",
            "UseSecretStore": true,
            "CPUsPerTask": 1,
            "Memory": 512,
            "TimeLimit": 900
        }
    },
    "DataStores": {
        "My_S3_Bucket": {
            "Endpoint": "https://s3.us-west-2.amazonaws.com",
            "Bucket": "faasr-bucket-0001",
            "Region": "us-west-2",
            "Writable": true
        }
    },
    "ActionList": {
        "start": {
            "FunctionName": "timestamp_logger",
            "FaaSServer": "GH",
            "Type": "Python",
            "Arguments": {
                "folder": "runtime_test",
                "tag": "start"
            },
            "InvokeNext": [
                "action_gcp",
                "action_lambda",
                "action_gh",
                "faasr-vm-poll-action_vm"
            ]
        },
        "action_gcp": {
            "FunctionName": "timestamp_logger",
            "FaaSServer": "GCP",
            "Type": "Python",
            "Arguments": {
                "folder": "runtime_test",
                "tag": "gcp"
            },
            "InvokeNext": [
                "faasr-vm-stop"
            ]
        },
        "action_lambda": {
            "FunctionName": "timestamp_logger",
            "FaaSServer": "GH",
            "Type": "Python",
            "Arguments": {
                "folder": "runtime_test",
                "tag": "aws"
            },
            "InvokeNext": [
                "faasr-vm-stop"
            ]
        },
        "action_gh": {
            "FunctionName": "timestamp_logger",
            "FaaSServer": "GH",
            "Type": "Python",
            "Arguments": {
                "folder": "runtime_test",
                "tag": "gh"
            },
            "InvokeNext": [
                "faasr-vm-stop"
            ]
        },
        "action_vm": {
            "FunctionName": "timestamp_logger",
            "FaaSServer": "GH",
            "Type": "Python",
            "RequiresVM": true,
            "Arguments": {
                "folder": "runtime_test",
                "tag": "vm"
            },
            "InvokeNext": [
                "faasr-vm-stop"
            ]
        },
        "faasr-vm-start": {
            "FunctionName": "vm_start",
            "FaaSServer": "GH",
            "Type": "Python",
            "RequiresVM": false,
            "InvokeNext": [
                "start"
            ],
            "_faasr_builtin": true
        },
        "faasr-vm-stop": {
            "FunctionName": "vm_stop",
            "FaaSServer": "GH",
            "Type": "Python",
            "RequiresVM": false,
            "InvokeNext": [],
            "_faasr_builtin": true
        },
        "faasr-vm-poll-action_vm": {
            "FunctionName": "vm_poll",
            "FaaSServer": "GH",
            "Type": "Python",
            "RequiresVM": false,
            "InvokeNext": [
                "action_vm"
            ],
            "_faasr_builtin": true
        }
    },
    "ActionContainers": {
        "start": "ghcr.io/faasr/github-actions-python:latest",
        "action_gcp": "faasr/gcp-r:latest",
        "action_lambda": "145342739029.dkr.ecr.us-east-1.amazonaws.com/aws-lambda-python:latest",
        "action_gh": "ghcr.io/faasr/github-actions-python:latest",
        "action_vm": "ghcr.io/faasr/github-actions-python:latest",
        "faasr-vm-poll-action_vm": "ghcr.io/faasr/github-actions-python:latest",
        "faasr-vm-start": "ghcr.io/faasr/github-actions-python:latest",
        "faasr-vm-stop": "ghcr.io/faasr/github-actions-python:latest"
    },
    "VMConfig": {
        "Name": "AWS_VM",
        "Provider": "AWS",
        "Strategy": "simple_start_end",
        "InstanceId": "i-09405009da063a647",
        "Region": "us-west-2",
        "RunnerName": "ip-172-31-39-220"
    },
    "FunctionGitRepo": {
        "timestamp_logger": "Ashish-Ramrakhiani/FaaSr-workflow-public"
    },
    "FunctionInvoke": "faasr-vm-start",
    "FaaSrLog": "FaaSrLog",
    "LoggingDataStore": "My_S3_Bucket",
    "DefaultDataStore": "My_S3_Bucket",
    "InvocationIDFromDate": "%Y%m%d%H%M",
    "WorkflowName": "cross_platform_benchmark"
}
